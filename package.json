{
  "name": "monorepo-starter",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "packages:build": "npm run build -w @monorepo-starter/dto -w @monorepo-starter/interfaces",
    "install": "npm run packages:build",
    "web:dev": "npm run serve -w @monorepo-starter/web",
    "server:dev": "npm run serve -w @monorepo-starter/server",
    "all:build": "npm run build --workspaces",
    "all:clean": "npm run clean --workspaces",
    "binary:build": "node scripts/build-binary.js"
  },
  "dependencies": {
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "rimraf": "3.0.2",
    "typescript": "4.6.3",
    "pkg": "^5.6.0"
  }
}
